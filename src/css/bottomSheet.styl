.q-bottom-sheet {
    border-radius: 30px 30px 0px 0px !important;
}

.q-bottom-sheet--grid .q-bottom-sheet__item {
    margin: 0 auto;
}

.q-card__section .q-dialog__message .scroll {
    text-align: center !important;
}

.q-dialog__message {
    text-align: center;
}

.q-dialog__message::after{
    content: '';
    border: 2px solid #000;
    border-radius: 6px;
    display: flex;
    width: 80px;
    margin: 0 auto;
}